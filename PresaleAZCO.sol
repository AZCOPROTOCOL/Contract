// File: presale.sol


pragma solidity ^0.8.20;



contract Presale {
    using SafeMath for uint256;

    address public owner;
    uint256 public tokenAZCO = 100; 
    IERC20 public token; 
    mapping(address => uint256) public balances;
    bool public isOpen = true;

    event TokensPurchased(address buyer, uint256 amount, uint256 cost);

    constructor(address _tokenAddress) {
        owner = msg.sender;
        token = IERC20(_tokenAddress);
    }

    modifier onlyOwner() {
        require(
            msg.sender == owner,
            "Solo el propietario puede ejecutar esta funcion."
        );
        _;
    }

    modifier isOpenPresale() {
        require(isOpen, "La venta anticipada esta cerrada.");
        _;
    }

    function transferTokensToPresale(uint256 _amount) external onlyOwner {
        token.transferFrom(msg.sender, address(this), _amount);
    }

    event PurchaseFailed(address buyer, uint256 amount, uint256 cost);

    function buyTokens(uint256 _etherAmount) external payable isOpenPresale {
        
        require(
            _etherAmount > 0 && _etherAmount == msg.value,
            "send ether."
        );

        
        uint256 tokenAmount = _etherAmount * tokenAZCO;

        
        token.transfer(msg.sender, tokenAmount);

        
        balances[msg.sender] += tokenAmount;

        
        emit TokensPurchased(msg.sender, tokenAmount, _etherAmount);
    }

    function transfer(address _to, uint256 _amount) external payable {
      
        uint256 tokensToTransfer = _amount.mul(tokenAZCO);

       
        require(
            msg.value >= _amount,
            "No send eth."
        );

        
        token.transferFrom(msg.sender, address(this), tokensToTransfer);

       
        token.transfer(_to, tokensToTransfer);

        
        if (msg.value > _amount) {
            payable(msg.sender).transfer(msg.value - _amount);
        }
    }

    function emergencyWithdraw() external onlyOwner {
       
        payable(owner).transfer(address(this).balance);

        
        uint256 contractBalance = token.balanceOf(address(this));
        if (contractBalance > 0) {
            token.transfer(owner, contractBalance);
        }
    }

    function withdrawTokensAzco() external onlyOwner {
        uint256 contractBalance = token.balanceOf(address(this));

       
        if (contractBalance > 0) {
            token.transfer(owner, contractBalance);
        }
    }

    function closePresale() external onlyOwner {
        isOpen = false;
    }

    
    function setTokenPrice(uint256 _tokenPrice) external onlyOwner {
        tokenAZCO = _tokenPrice;
    }

    
    function withdrawTokens(address _to, uint256 _amount) external onlyOwner {
        require(
            token.balanceOf(address(this)) >= _amount,
            "not tokens  "
        );
        token.transfer(_to, _amount);
    }
}
